# Next.js build directory
.next

# Node.js dependencies directory
node_modules

# Public directory with static files like images and fonts
public

# Package lock files (Yarn and npm)
yarn.lock
package-lock.json

# Automatically generated environment file by Next.js
next-env.d.ts

# Log files generated during development or runtime
*.log
logs

# Cache files generated by development tools
.cache
.eslintcache

# Test coverage reports
coverage

# Local build directories (production or development builds)
dist
build

# Environment files with sensitive variables or configurations
.env*